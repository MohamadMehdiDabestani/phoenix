// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  previewFeatures = ["dataProxy"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  
}

model User {
  id String @id @default(uuid())
  userName String
  email String @unique
  registerDate String
  lastLogin String?
  password String
  isActive Boolean @default(false)
  loginKey String? @unique
  botStrategy Json?
}

model Trades {
  id String @id @default(uuid())
  symbol String
  entry Float
  stop Float
  status String
  liquidPrice Float
  profit Float
  afterBalance Float
  side String
  strategy String
}
model TradingProfile {
  id String @id @default(uuid())
  balance Float
  botStrategy Json
}